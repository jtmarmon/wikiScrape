{
  "manifest_version": 2,

  "name": "wikiScrape",
  "description": "Scrape wikipedia articles for PDFs.",
  "version": "1.0",

  "permissions": [
    "https://google.com/*",
    "https://wikipedia.org/*"
  ],

  "browser_action": {
    "default_icon": "cat.png",
    "default_popup": "toggle.html"
  }
}